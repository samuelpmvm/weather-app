msgstr "Previsão do tempo para {city}:"

msgid "{dt_txt}: {temp}°C, {desc}, Humidity: {humidity}%, Wind: {wind_speed} m/s"
msgstr "{dt_txt}: {temp}°C, {desc}, Humidade: {humidity}%, Vento: {wind_speed} m/s"

